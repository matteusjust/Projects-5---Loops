/* finds the greatest common denominator out of two numbers */

#include <stdio.h>

int main(void)
{
    int m, n, remain;
    
    printf ("Insert two numbers: ");
    scanf ("%d %d", &m, &n);
    
    while (n != 0){
        remain = m % n;
        m = n;
        n = remain;
     
    }
    printf ("The greatest common denominator is : %d", m);
    
    return 0;
}
